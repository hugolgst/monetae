{
  "version": 1,
  "dfx": "0.8.4",
  "canisters": {
    "monetae": {
      "type": "rust",
      "package": "monetae",
      "candid": "src/monetae/monetae.did"
    },
		"monetae-frontend": {
			"dependencies": [
        "monetae"
      ],
      "frontend": {
        "entrypoint": "out/index.html"
      },
      "source": [
        "out/"
      ],
      "type": "assets",
			"build": "npm run export"
		}
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
		"ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  }
}
