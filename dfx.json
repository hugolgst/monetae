{
	"version": 1,
	"dfx": "0.8.4",
	"canisters": {
		"contract": {
			"type": "custom",
			"package": "contract",
			"candid": "src/contract/contract.did",
			"wasm": "target/wasm32-unknown-unknown/release/contract_opt.wasm",
			"build": "./build.sh"
		},
		"frontend": {
			"frontend": {
				"entrypoint": "src/frontend/dist/index.html"
			},
			"source": [
				"src/frontend/dist/"
			],
			"type": "assets"
		}
	},
	"defaults": {
		"build": {
			"packtool": "",
			"args": ""
		}
	},
	"networks": {
		"local": {
			"bind": "127.0.0.1:8000",
			"type": "ephemeral"
		},
		"ic": {
			"providers": [
				"https://mainnet.dfinity.network"
			],
			"type": "persistent"
		}
	}
}
